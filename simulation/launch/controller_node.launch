<launch>
    <node name="gp4_lego_bringup" pkg="gp4_lego" type="gp4_lego_bringup" output="screen" required="true">
        <param name="config_fname" value="$(find gp4_lego)/config/user_config.json"/>
        <param name="root_pwd" value="$(find gp4_lego)/"/>
        <remap from="/gp4_lego_bringup/j1_pub" to="/gp4_gazebo/joint1_position_controller/command"/>
        <remap from="/gp4_lego_bringup/j2_pub" to="/gp4_gazebo/joint2_position_controller/command"/>
        <remap from="/gp4_lego_bringup/j3_pub" to="/gp4_gazebo/joint3_position_controller/command"/>
        <remap from="/gp4_lego_bringup/j4_pub" to="/gp4_gazebo/joint4_position_controller/command"/>
        <remap from="/gp4_lego_bringup/j5_pub" to="/gp4_gazebo/joint5_position_controller/command"/>
        <remap from="/gp4_lego_bringup/j6_pub" to="/gp4_gazebo/joint6_position_controller/command"/>
    </node>
</launch>