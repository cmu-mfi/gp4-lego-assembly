<launch>
	<arg name="namespace" />
    <group ns="$(arg namespace)">
	    
    <node name="gp4_lego_task_planning_cartesian" pkg="gp4_lego" type="gp4_lego_task_planning_cartesian" output="screen" required="true">
        <param name="config_fname" value="$(find gp4_lego)/config/user_config.json"/>
        <param name="root_pwd" value="$(find gp4_lego)/"/>
        <remap from="/sim/gp4_lego_task_planning_cartesian/robot_state" to="/sim/gp4_lego_bringup/robot_state"/>
        <remap from="/sim/gp4_lego_task_planning_cartesian/goal" to="/sim/gp4_lego_bringup/robot_goal"/>
    </node>

    </group>
</launch>
